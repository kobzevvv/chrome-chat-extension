{
    "manifest_version": 3,
    "name": "HH.ru Resume Links Extractor",
    "version": "1.0.0",
    "description": "Extract resume links from HH.ru vacancy responses",
    "permissions": [
      "activeTab",
      "storage",
      "scripting",
      "tabs"
    ],
    "host_permissions": [
      "*://*.hh.ru/*"
    ],
    "action": {
      "default_popup": "popup/popup.html",
      "default_title": "Extract Resume Links"
    }
  }